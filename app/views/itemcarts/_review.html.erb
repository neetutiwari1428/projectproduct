              </div>
              <div class="col-sm-3 col-xs-3 text-right">
                <h6><span>$</span><%= i&.product&.price %></h6>
              <p><%= link_to "Remove",remove_item_itemcart_path(i) %></p>
              <p><%#= link_to "Remove","/products/#{i.product_id}/itemcarts", method: :delete %></p>
              <p><%#= link_to "Remove", @itemcart, method: :delete %></p>
              <p><%= link_to "Order",orders_itemcart_path(i) %></p>
              <p><%#= link_to "Wishlist", wishlists_product_path(i) %></p>
              <a href="/products/<%= i.product.id %>/wishlists">Wishlist</a>
