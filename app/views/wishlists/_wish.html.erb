<div class="container wrapper">
  <div class="row cart-body">
    <form class="form-horizontal" method="post" action="">
      <div
        class=""
      >
        <!--REVIEW ORDER-->
        <div class="panel panel-info">
          <div class="panel-heading">
            Review Order
          </div>
          <div class="panel-body">
          <% @wishlists.each do |w| %>
            <div class="form-group">
              <div class="col-sm-3 col-xs-3">
                <img
                  class="img-responsive"
                  src="<%= w.product.image %>"
                />
              </div>
              <div class="col-sm-6 col-xs-6">
                <div class="col-xs-12"><a href=""><%= w&.product&.name %></div>
                <div class="col-xs-12">
                  <%# <small>Quantity:<span></span></small> %>
                  <%# <small>Size:<span></span></small> %>
                </div>
              </div>
              <div class="col-sm-3 col-xs-3 text-right">
                <h6><span>$</span><%= w&.product&.price %></h6>
              <p><%= link_to "Remove",remove_item_itemcart_path(w) %></p>
              <p><%#= link_to "Remove","/products/#{i.product_id}/itemcarts", method: :delete %></p>
              <p><%#= link_to "Remove", @itemcart, method: :delete %></p>
              <p><%= link_to "Order",orders_itemcart_path(w) %></p>
              </div>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>
